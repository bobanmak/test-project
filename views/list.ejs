<h1 class="display-4">Notes</h1>
<table class="table table-striped">
    <thead>
        <tr>
            <th>#id</th>
            <th>#title</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        <% notes.forEach(function(notes){ %>
            <tr>
                <td><%= notes.id %></td>
                <td><%= notes.title %></td>
                <td>
                    <a href="/notes/edit/<%= notes.id %>" class="btn btn-primary">Edit</a>
                    <a href="/notes/view/<%= notes.id %>" class="btn btn-info">Single View</a>                    
                    <form action="/notes/delete/<%= notes.id %>" method="POST" class="d-inline">
                    <input type="submit" value="Delete" class="btn btn-danger">
                    </form>
                </td>
            </tr>
        
        <% }) %>
    </tbody>
</table>